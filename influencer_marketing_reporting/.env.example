# Google Drive API credentials
# Need to run: gcloud auth application-default login --scopes=https://www.googleapis.com/auth/drive
GOOGLE_DRIVE_FOLDER_ID=your_folder_id_here

# Webhook configuration
WEBHOOK_SECRET=your_webhook_secret_for_verification 

# Public URL for webhook registration 
# For local development, use a service like ngrok to expose your local server
# Format: https://your-subdomain.ngrok.io (no trailing slash)
WEBHOOK_PUBLIC_URL=your_ngrok_url_here